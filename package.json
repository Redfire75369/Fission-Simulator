{
	"name": "fission-simulator",
	"version": "0.6.12",
	"description": "Fission Simulator is an incremental game about nuclear fission, reactors and nuclear physics.",
	"main": "javascripts/server.js",
	"scripts": {
		"setup": "npm ci",
		"compile": "npx --no-install babel javascripts/react/dev --out-dir javascripts/react/dist",
		"eslint": "eslint \"javascripts/**/*\" --ext .js,.jsx || true",
		"csslint": "csslint . --exclude-list=stylesheets/katex.css || true",
		"lint": "npm run eslint && npm run csslint",
		"build": "npm run lint && npm run compile  ",
		"count": "git ls-files | xargs wc -l || true"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Redfire75369/Fission-Simulator.git"
	},
	"author": "Redfire",
	"license": "UNLICENSED",
	"bugs": {
		"url": "https://github.com/Redfire75369/Fission-Simulator/issues"
	},
	"homepage": "https://github.com/Redfire75369/Fission-Simulator#readme",
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/preset-react": "^7.10.4",
		"babel-loader": "^8.1.0",
		"csslint": "^1.0.5",
		"eslint": "^7.8.1",
		"eslint-plugin-react": "^7.20.6"
	},
	"dependencies": {}
}
